<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="400.0" prefWidth="624.0"
            xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
            fx:controller="sample.Controller">
    <children>
        <TableView fx:id="tableObject" layoutX="25.0" layoutY="25.0" prefHeight="350.0" prefWidth="600.0" >
            <columns>
                <TableColumn prefWidth="300.0" text="Object" fx:id="objColumn" />
                <TableColumn prefWidth="300.0" text="Hash code" fx:id="hashCodeColumn" />
            </columns>
        </TableView>
        <Button layoutX="25.0" layoutY="400.0" onAction="#addClick" text="Add new"/>
        <Button layoutX="300.0" layoutY="400.0" text="Edit" onAction="#editClick"/>
        <Button layoutX="560.0" layoutY="400.0" text="Delete" onAction="#deleteClick"/>
    </children>
</AnchorPane>
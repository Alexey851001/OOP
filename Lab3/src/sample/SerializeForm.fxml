<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.SerializeController"
            prefHeight="400.0" prefWidth="600.0">
    <Label text="Serialize" layoutX="20.0" layoutY="20.0" />
    <Button layoutX="20.0" layoutY="50.0" text="binary" onAction="#binarySerialize"/>
    <Button layoutX="20.0" layoutY="100.0" text="XML" onAction="#XMLSerialize"/>
    <Button layoutX="20.0" layoutY="150.0" text="text" onAction="#textSerialize"/>
    <Label text="Deserialize" layoutX="220.0" layoutY="20.0" />
    <Button layoutX="220.0" layoutY="50.0" text="binary" onAction="#binaryDeserialize"/>
    <Button layoutX="220.0" layoutY="100.0" text="XML" onAction="#XMLDeserialize"/>
    <Button layoutX="220.0" layoutY="150.0" text="text" onAction="#textDeserialize"/>
</AnchorPane>
